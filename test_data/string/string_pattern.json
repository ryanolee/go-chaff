{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "test_fixed_length": {
            "type": "string",
            "pattern": "^\\d{3}$"
        },
        "test_whitespace": {
            "type": "string",
            "pattern": "^\\s+$"
        },
        "test_no_whitespace": {
            "type": "string",
            "pattern": "^\\S+$"
        },
        "test_alphanumeric": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9]+$"
        },
        "test_alphabetic": {
            "type": "string",
            "pattern": "^[a-zA-Z]+$"
        },
        "test_numeric": {
            "type": "string",
            "pattern": "^\\d+$"
        },
        "test_fixed_string": {
            "type": "string",
            "pattern": "^abc$"
        },
        "test_alternate": {
            "type": "string",
            "pattern": "^(a|b)$"
        },
        "test_complex": {
            "type": "string",
            "pattern": "^.*[a-b]{0,}932423{5}(a|b)$"
        }
    }
}